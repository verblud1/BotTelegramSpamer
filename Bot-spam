@bot.message_handler(content_types = ['text'])
def main(message):
    id = message.chat.id
    msg = message.text

    if msg == 'Ğ¿Ñ€':
        send(id,'df')
    if msg =='df':
        send(id,'Ğ¿Ñ€')
    if msg == 'Ğ°Ğ°':
        send(id, 'Ğ½ÑƒĞ´Ğ°')
    else:
        send(id,"Ñ‡Ğµ")
bot.polling(none_stop = True)
